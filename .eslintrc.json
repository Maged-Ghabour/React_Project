{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:react-hooks/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react" , "jsx-a11y" ,"import"
    ],
    "rules": {
        "react/prop-types" : 0 , 
        "react/react-in-jsx-scope" : 0
    } , 
    "settings": {
        "react" : {
        "version" : "detect"
        }, 
        "import/resolver" : {
            "node" : {
                "extensions" : [".js" , ".jsx"]
            }
        }
    }
}
